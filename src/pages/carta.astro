---
import Layout from "../layouts/Layout.astro";
import Navbar from "../components/public/Navbar.astro";
import CartaAutor from "../components/public/CartaAutor.astro";
import CartaShot from "../components/public/CartaShot.astro";
import CartaMixologia from "../components/public/CartaMixologia.astro";
import CartaCeroAlcohol from "../components/public/CartaCeroAlcohol.astro";
import CartaClasica from "../components/public/CartaClasica.astro";
import Redes from "../components/public/Redes.astro";
import Footer from "../components/public/Footer.astro";
---

<Layout>
    <Navbar slot={"navbar"} />
    <CartaAutor slot={"carta-autor"} />
    <CartaShot slot={"carta-shot"} />
    <CartaMixologia slot={"carta-mixologia"} />
    <CartaCeroAlcohol slot={"carta-cero-alcohol"} />
    <CartaClasica slot={"carta-clasica"} />
    <Redes slot="redes" />
    <Footer slot={"footer"} />
</Layout>